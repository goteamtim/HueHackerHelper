{
  "name": "Hue Hacker",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Helping you get your developer keys faster and easier so you can focus on your code!",
  "homepage_url": "http://www.timharshman.com/HueHackerHelper",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "Hue Hacker",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "background": {
    "scripts": ["/js/background.js"]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "storage"
  ]
}